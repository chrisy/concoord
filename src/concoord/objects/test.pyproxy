import clientproxy
'\n@author: Deniz Altinbuken, Emin Gun Sirer\n@note: Example test object\n@date: March 20, 2011\n@copyright: See COPYING.txt\n'

class Test:

    def __init__(self, bootstrap, **kwargs):
        self.proxy = clientproxy.ClientProxy(bootstrap)

    def getvalue(self, **kwargs):
        return self.proxy.invoke_command('getvalue')

    def __str__(self, **kwargs):
        return self.proxy.invoke_command('__str__')