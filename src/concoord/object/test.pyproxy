from concoord.clientproxy import ClientProxy
'\n@author: Deniz Altinbuken, Emin Gun Sirer\n@note: Example test object\n@copyright: See LICENSE\n'

class Test:

    def __init__(self, bootstrap=None, connect=False, **kwargs):
        if (connect and (not bootstrap)):
            raiseConCoordException('bootstrap cannot be None')
        elif (connect and bootstrap):
            self.proxy = ClientProxy(bootstrap)
        else:
            return self.proxy.invoke_command('__init__')

    def getvalue(self, **kwargs):
        return self.proxy.invoke_command('getvalue')

    def setvalue(self, newvalue, **kwargs):
        return self.proxy.invoke_command('setvalue', newvalue)

    def __str__(self, **kwargs):
        return self.proxy.invoke_command('__str__')